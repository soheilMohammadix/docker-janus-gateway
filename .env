# Janus Gateway Configuration
# Copy this file to .env and modify the values as needed

# Network Configuration
GATEWAY_IP=192.168.1.100  # Replace with your actual local IP address

# Security
ADMIN_SECRET=janusoverlord
VIDEOROOM_ADMIN_KEY=supersecret
STREAMING_ADMIN_KEY=supersecret
RABBITMQ_PASSWORD=janus123
REDIS_PASSWORD=redis123

# Graylog Configuration (optional)
GRAYLOG_PASSWORD_SECRET=somepasswordpepper
GRAYLOG_ROOT_PASSWORD_SHA2=8c6976e5b5410415bde908bd4dee15dfb167a9c873fc4bb8a81f6f2ab448a918

# Development Settings
DEBUG_LEVEL=4
DEBUG_COLORS=true
DEBUG_LOCKS=false

# Media Configuration
RTP_PORT_RANGE=10000-10099
DTLS_MTU=1200
NO_MEDIA_TIMER=1
SLOWLINK_THRESHOLD=4
TWCC_PERIOD=200

# ICE Configuration
NICE_DEBUG=false
FULL_TRICKLE=true
ICE_LITE=false
ICE_TCP=false
ICE_ENFORCE_LIST=
ICE_IGNORE_LIST=vmnet

# WebSocket Configuration
WEBSOCKETS_ENABLED=true
WEBSOCKETS_EVENTS=true
WEBSOCKETS_JSON=indented
WEBSOCKETS_PORT=8189
WEBSOCKETS_PINGPONG_TRIGGER=30
WEBSOCKETS_PINGPONG_TIMEOUT=5

# Recording Configuration
RECORDINGS_TMP_EXT=tmp
RECORDINGS_DIR=/opt/janus/share/janus/recordings